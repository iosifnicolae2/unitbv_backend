<!DOCTYPE html>
<html lang="en">
  <head>
    <% include parts/head_content.ejs %>
    <style>
    body { background-image: url(data:image/png;base64,);}
.error-template {padding: 40px 15px;text-align: center;}
.error-actions {margin-top:15px;margin-bottom:15px;}
.error-actions .btn { margin-right:10px; }
    </style>
  </head>

  <body>

    <div class="container">

      <% include parts/navbar.ejs %>


      <div class="container">
          <div class="row">
              <div class="col-md-12">
                  <div class="error-template">
                      <h1>Oops! An error occurred</h1>
                      <pre class="error-details">
                          <% if(typeof error != 'undefined' && error !=null && typeof error.stack != 'undefined'){%><%=error.stack%><%}%>
                      </pre>
                      <pre class="reason-details">
                        <% if(typeof error != 'undefined' && error !=null && typeof error.errors != 'undefined'){%><%-JSON.stringify(error.errors, null, "\t")%><%}%>
                      </pre>
                      <div class="error-actions">
                          <a href="#" onclick="goBack()" class="btn btn-primary btn-lg"><span class="glyphicon glyphicon-home"></span>
                              Take Me Back </a><a href="mailtio:iosifnicolae2@gmail.com" class="btn btn-default btn-lg"><span class="glyphicon glyphicon-envelope"></span> Contact Support.</a>
                      </div>
                  </div>
              </div>
          </div>
      </div>


    </div> <!-- /container -->



          <% include parts/scripts.ejs %>
          <script>
function goBack() {
    window.history.back();
}
</script>
  </body>
</html>

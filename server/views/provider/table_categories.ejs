<% if(typeof table_categories !='undefined'){%>
  <% table_categories.forEach(function(category,i){%>
<tr>
    <td><%=i%></td>
    <td><%=category.name%></td>
    <td><%=category.short_description%></td>
    <td>
      <img class="business_logo" src="<%=category.cdn_logo%>"/></td>
    <td class="text-center">
      <a class='btn btn-info btn-xs' href="/categories/edit/<%=category._id%>"><span class="glyphicon glyphicon-edit"></span> Edit</a>
       <a href="/categories/remove/<%=category._id%>" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span> Del</a>
     </td>
</tr>

<%})}%>

<% if(typeof table_posts !='undefined'){%>
  <% table_posts.forEach(function(post,i){%>
<tr>
    <td><%=i%></td>
    <td><%=post.admin_id%></td>
    <td><%=post.content%></td>
    <td><%=post.poi_id%></td>
    <td>
      <img class="post_logo" src="<%=post.cdn_logo%>"/></td>
    <td class="text-center">
      <a class='btn btn-info btn-xs' href="/post/edit/<%=post._id%>"><span class="glyphicon glyphicon-edit"></span> Edit</a>
       <a href="/post/remove/<%=post._id%>" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span> Del</a>
     </td>
</tr>

<%})}%>
